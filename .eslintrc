{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "comma-dangle": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "new-cap": "off",
        "max-len": "off",
        "operator-linebreak": "off",
        "no-unused-vars": "off",

        //delete after
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "indent": "off",
        "space-before-function-paren": "off",
        
    },
    "extends": ["eslint:recommended", "google"]
}